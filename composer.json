{
  "name": "intelligentspark/insert-tag-event-title-bundle",
  "description": "Frontend Event Title Insert Tag Bundle for Contao 4",
  "type": "contao-bundle",
  "authors": [
    {
      "name": "Fred Bliss",
      "email": "fred@intelligentspark.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "contao/core-bundle": "^4.4",
    "contao/calendar-bundle": "^4.4"
  },
  "extra": {
    "contao-manager-plugin": "IntelligentSpark\\EventTitleInsertTag\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "IntelligentSpark\\EventTitleInsertTag\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  }
}
